#include <numeric>
#include <sstream>
#include <iostream>
#include <algorithm>
#include <unordered_map>

using namespace std;


int main(){
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++){
        long n;
        cin >> n;
        long long a2=1,a1=2;
        long long sum=0;
        while(a1<=n)
        {
            if(a1%2==0)
            sum+=a1;
            long long temp=a2+a1;
            a2=a1;
            a1=temp;
            
        }
        
        cout<<sum<<endl;
    }
    return 0;
}

